# Example .env file for Local Deep Researcher
# Copy this file to .env and adjust as needed

# LLM model to use with Ollama
LOCAL_LLM=qwen3:14b

# Web search API (default: duckduckgo)
SEARCH_API=duckduckgo

# Maximum number of research loop steps (default: 3)
MAX_WEB_RESEARCH_LOOPS=3

# Fetch the full page content (default: true)
FETCH_FULL_PAGE=true
